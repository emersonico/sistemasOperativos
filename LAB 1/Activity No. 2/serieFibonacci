#include <stdio.h>

int main()
{
    int n, primero = 0, segundo = 1, siguiente;
    
    printf("Digite el numero de terminos: ");
    scanf("%d", &n);
    
    printf("La serie de Fibonacci: \n");
    
    for (int i = 0; i < n; i++)
    {
        if (i <= 1){
            siguiente = i;
        }    
        else {
            siguiente = primero + segundo;
            primero = segundo;
            segundo = siguiente;
        }
        
        printf("%d\n", siguiente);
    }
   
   return 0;
}